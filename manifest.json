{
	"manifest_version": 2,
	"name": "AhaNamuWikiChromeExtension",
	"description": "AhaNamuWiki Chrome Extensios. - Inlines footnotes in namu.wiki.",
	"version": "0.1.0",
	"icons": {
		"48": "icon.png"
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"https://namu.wiki/*"
	],
	"content_scripts": [
		{
			"matches": [
				"https://namu.wiki/*"
			],
			"run_at": "document_idle",
			"js": [
				"jquery.min.js", "inline.js"
			]
		}
	]
}
